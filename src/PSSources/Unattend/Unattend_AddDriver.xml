    <cpi:offlineImage cpi:source="wim:d:/_dev/projetos/management-deployment-tools/deployment/images/wim/26100.2033/default/sources/install.wim#Windows 11 Pro" xmlns:cpi="urn:schemas-microsoft-com:cpi" />
